button{
    overflow: hidden;
}
.confirmButton{
    width: 408rpx;
    height: 86rpx;
    margin: auto;
    background: #F5C2BF;
    border: 1px solid #F8C1B0;
    box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
    display: flex;
    align-items: center;
    justify-content: center;
    font-style: normal;
    font-weight: bold;
    font-size: 30rpx;
    line-height: 30rpx;
    padding: 5rpx;
}

.wechat-logo{
    margin-left: 10rpx;
    width: 36rpx;
    height: 36rpx;
}

.confirm-text{
    font-family: Heiti SC;
    font-style: normal;
    font-weight: bold;
    font-size: 30rpx;
    line-height: 30rpx;
    display: flex;
    align-items: center;
    text-align: center;

    color: #953A3A;
}