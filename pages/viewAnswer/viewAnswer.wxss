.icon{
  display: inline-block;
  width: 28rpx;
  height: 28rpx
}

.delete{
  position: absolute;
  right: 24rpx;
  bottom: 24rpx;   
  width: 60rpx;
  height: 60rpx;
}

.invisible{
  display: none;
}

.bottom {
  position: relative;
  display: flex;
  height: 88rpx;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  padding-right: 80rpx;
}
.bottom view {
  position: relative;
}

.bottom view image {
  width: 40rpx;
  height: 38rpx;
}
.bottom view text {
  color: #6A6868;
  font-size: 26rpx;
}

.date-text{
  width: 104rpx;
  height: 26rpx;

  font-family: Heiti SC;
  font-style: normal;
  font-weight: normal;
  font-size: 18rpx;
  line-height: 18rpx;
  display: flex;
  align-items: center;
  text-align: center;

  color: #A8A8A8;
}

.question-text{
  margin-left: 14rpx;
  margin-right: 14rpx;
  height: auto;
  
  font-family: Heiti SC;
  font-style: normal;
  font-weight: bold;
  font-size: 32rpx;
  line-height: 32rpx;
  align-items: center;
  color: #953A3A;
}

button{
  width: 330rpx;
  height: 74rpx;
  margin-top: 50rpx;

  background: #FFC61B;
  border: 2rpx solid #F8C1B0;
  box-sizing: border-box;
  box-shadow: 0rpx 8rpx 8rpx rgba(0, 0, 0, 0.25);  
}

button text{
  margin-left: 104rpx;
  width: 124rpx;
  height: 30rpx;
  
  font-family: Heiti SC;
  font-style: normal;
  font-weight: bold;
  font-size: 30rpx;
  line-height: 30rpx;
  display: flex;
  align-items: center;
  text-align: center;

  color: #953A3A;
}

textarea{
  width: 278rpx;
  margin-left: 18rpx;
  margin-right: 18rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  
  font-family: Heiti SC;
  font-style: normal;
  font-weight: normal;
  font-size: 13px;
  line-height: 13px;
  align-items: center;
  color: #6A6868;

  overflow: auto;
  word-wrap: normal;
}

.show-btn {
  margin-top: 100rpx;
  color: #22cc22;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 600rpx;
  margin-left: 75rpx;
  margin-right: 112rpx;
  overflow: hidden;
  position: fixed;
  top: 10%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  border-radius: 36rpx;
}

.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}

.modal-content {
  padding: 50rpx 32rpx;
}

.modal-input {
  display: flex;
  background: #fff;
  border: 2rpx solid #ddd;
  border-radius: 4rpx;
  font-size: 28rpx;
}

.input {
  width: 100%;
  height: 82rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #333;
}

input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 2rpx solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 2rpx solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #953A3A;
  text-align: center;
}