.mask {
  z-index: -1;
  position: absolute;
  top: 275rpx;
  bottom: -200rpx;
  left: 0;
  right: 0;
  background-color: #FFF7DB;
}
.login_wrapper {
  width: 410rpx;
  height: 176rpx;
  margin: auto;
  margin-top: 320rpx;
  text-align: center;
}
.login_wrapper .login_text {
  padding-top: 32rpx;
  padding-left: 5rpx;
  font-size: 44rpx;
  line-height: 45rpx;
  text-align: center;
  color: #953A3A;
}
.login_wrapper .login_line {
  width: 100%;
  border: 1px solid #F8C1B0;
  margin-bottom: 12rpx;
  margin-top: 12rpx;
}
.login_wrapper image {
  width: 264rpx;
  height: 34rpx;
  margin: auto;
}
.login_wrapper button {
  background-color: #F5C2BF;
  margin-top: 110rpx;
  width: 450rpx;
  height: 86rpx;
  font-size: 28rpx;
  font-weight: 400;
  font-style: normal;
  display: flex;
  align-items: center;
}
.login_wrapper button image {
  width: 36rpx;
  height: 36rpx;
}
.intro{
  font-size: 20rpx;
  color:#6A6868;
}
.intro navigator{
  display: inline;
  text-decoration:underline;
  color:#953A3A;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 600rpx;
  margin-left: 75rpx;
  margin-right: 112rpx;
  overflow: hidden;
  position: fixed;
  top: 30%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  border-radius: 36rpx;
  border: 2rpx solid #F8C1B0;
  box-sizing: border-box;
  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
}

.modal-title {
  padding-top: 50rpx;

  font-family: Heiti SC;
  font-style: normal;
  font-weight: bold;
  font-size: 34rpx;
  line-height: 36rpx;
  align-items: center;
  text-align: center;

  color: #953A3A;
}

.modal-content {
  padding: 50rpx 32rpx;
  text-align: left;
}

.modal-input {
  display: flex;
  background: #FFF7DB;
  border: 2rpx solid #F8C1B0;
  border-radius: 4rpx;
  font-size: 28rpx;
  box-sizing: border-box;
}

.input {
  width: 100%;
  height: 82rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #333;
}

input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 2rpx solid #F8C1B0;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-confirm {
  width: 100%;
  color: #953A3A;
  text-align: center;
}